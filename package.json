{
  "name": "mojang-lib",
  "version": "1.3.0",
  "description": "A modern wrapper for interacting with the Mojang API.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "type": "module",
  "scripts": {
    "test": "npx tsx test.ts",
    "prebuild": "rimraf dist",
    "build": "npm run prebuild && npm run build:esm && npm run build:cjs",
    "build:cjs": "tsc --module CommonJS --outDir dist/cjs",
    "build:esm": "tsc"
  },
  "funding": "https://github.com/sponsors/Owen3H",
  "repository": {
    "type": "git",
    "url": "https://github.com/Owen3H/mojang-lib.git"
  },
  "keywords": [
    "mojang", 
    "minecraft", 
    "minecraft-lib", 
    "mojang-api", 
    "typescript",
    "api-wrapper",
    "node"
  ],
  "author": "Owen77Stubbs",
  "license": "MIT",
  "dependencies": {
    "tslib": "^2.5.3",
    "undici": "latest"
  },
  "devDependencies": {
    "@types/node": "^20.3.1",
    "rimraf": "^5.0.1"
  }
}
