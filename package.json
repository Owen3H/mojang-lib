{
  "name": "mojang-lib",
  "version": "1.4.0",
  "description": "A modern wrapper for interacting with the Mojang API.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files:": [
    "dist",
    "README.md",
    "CHANGES.md",
    "LICENSE"
  ],
  "scripts": {
    "test": "cd test-pkg && npm run test",
    "prebuild": "rimraf dist",
    "build": "npm run prebuild && npm run build:esm && npm run build:cjs",
    "build:cjs": "tsc --module CommonJS --outDir dist/cjs",
    "build:esm": "tsc",
    "tsup": "tsup src/index.ts --format esm,cjs --legacy-output"
  },
  "funding": "https://github.com/sponsors/Owen3H",
  "repository": {
    "type": "git",
    "url": "https://github.com/Owen3H/mojang-lib.git"
  },
  "keywords": [
    "mojang",
    "minecraft",
    "minecraft-lib",
    "mojang-api",
    "typescript",
    "api-wrapper",
    "node"
  ],
  "author": "Owen77Stubbs",
  "license": "MIT",
  "dependencies": {
    "tslib": "^2.5.3",
    "undici": "latest"
  },
  "devDependencies": {
    "@types/node": "^20.3.1",
    "rimraf": "^5.0.1"
  }
}
