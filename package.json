    "rollup": "rollup -c",
    "typedoc": "npx typedoc --options typedoc.json"
  },
  "funding": "https://github.com/sponsors/Owen3H",
  "repository": {
    "type": "git",
    "url": "https://github.com/Owen3H/mojang-lib.git"
  },
  "keywords": [
    "mojang",
    "minecraft",
    "minecraft-lib",
    "mojang-api",
    "typescript",
    "api-wrapper",
    "node"
  ],
  "author": "Owen77Stubbs",
  "license": "MIT",
  "dependencies": {
    "tslib": "^2.5.3",
    "undici": "latest"
  },
  "devDependencies": {
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "^8.43.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "rimraf": "^5.0.1",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "typedoc": "^0.24.8",
    "typedoc-theme-hierarchy": "^4.0.0",
    "typescript": "^5.1.6"
  }
