{
  "name": "mojang-lib",
  "version": "1.8.0",
  "description": "A modern wrapper for interacting with the Mojang API.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/types.d.ts",
  "files": [
    "dist",
    "README.md",
    "CHANGES.md",
    "LICENSE"
  ],
  "scripts": {
    "test": "cd test-pkg && npm run test",
    "build": "npm run clean && npm run rollup && npm run typedoc",
